import java.util.Scanner;

public class Question {
    public static void main(String[] args) {
        Scanner sc  = new Scanner(System.in);
        int x = sc.nextInt();
        int y = sc.nextInt();
        int Question=x/y;
        int remender=x%y;
        System.out.println("Question: "+Question);
        System.out.println("Remender: "+remender);
    }
}
